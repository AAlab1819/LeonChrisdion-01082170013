#include <iostream.h>

int main()
{
    int m, n, r;

    cout << "Masukan Bilangan 1 : ";
    cin >> m;

    cout << "Masukan Bilangan 2 : ";
    cin >> n;

    r = m % n;
    while(r != 0)
    {
        m = n;
        n = r;
        r = m % n;
    }

    cout<< "FPB bilangan 1 dan 2 adlah: " << n << endl;

    return 0;
}
